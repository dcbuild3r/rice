st.alpha: 0.85

/* Set a default font and font size as below:*/
st.font: Monospace-13;

st.termname: st-256color
!!st.borderpx: 2

#define base_foreground #fffaf4
#define base_background #0e1019
#define base_color0 	#232323
#define base_color1 	#ff000f
#define base_color2 	#8ce10b
#define base_color3 	#ffb900
#define base_color4 	#008df8
