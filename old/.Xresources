*foreground: base_foreground
*background: base_background
!black
*color0:  base_color0
*color8:  base_color8
!red
*color1:  base_color1
*color9:  base_color9
!green
*color2:  base_color2
*color10: base_color10
!yellow
*color3:  base_color3
*color11: base_color11
!blue
*color4:  base_color4
*color12: base_color12
!magenta
*color5:  base_color5
*color13: base_color13
!cyan
*color6:  base_color6
*color14: base_color14
!white
*color7:  base_color7
*color15: base_color15
*color15: base_color15


#define base_custom_foreground 		#d3dae3
#define base_custom_background_dark 	#404552
#define base_custom_background_light 	#666a73
#define base_custom_primary 		#3daee9
#define base_custom_warn 		#da4453



*custom_foreground: 		base_custom_foreground
*custom_background_dark: 	base_custom_background_dark
*custom_background_light: 	base_custom_background_light
*custom_primary: 		base_custom_primary
*custom_warn:			base_custom_warn

